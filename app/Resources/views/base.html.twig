<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome to Freedom!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('public/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('public/css/bootstrap-theme.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('public/css/jquery-ui.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('public/css/jquery-ui.structure.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('public/css/jquery-ui.theme.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('public/css/jquery-te-1.4.0.css') }}" />
            <link rel="stylesheet" href="{{ asset('public/css/style.css') }}" />
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
            <script type="text/javascript" src="https://code.jquery.com/jquery-1.11.1.min.js"></script>
            <script type="text/javascript" src="{{ asset('public/js/bootstrap.js') }}"></script>
            <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.23/angular.min.js"></script>
            <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular-resource.min.js"></script>
            <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular-animate.min.js"></script>
            <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.0/angular-sanitize.min.js"></script>
            <script type="text/javascript" src="{{ asset('public/js/jquery-ui.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/jquery-te-1.4.0.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/freedom.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/app.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/services.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/filters.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/directives.js') }}"></script>
            <script type="text/javascript" src="{{ asset('public/js/controllers.js') }}"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body ng-app="ObjectiveApp">
        {{render(controller("FreedomMenuBundle:Default:menuTop"))}}
        <div class="container content-page">
            {% block body %}{% endblock %}
            <hr>
        </div> <!-- /container -->
        <footer class="text-center">
            <p>&copy; 2013 - {{ "now"|date("Y") }} by Maxime Plancke</p>
        </footer>
    </body>
</html>
